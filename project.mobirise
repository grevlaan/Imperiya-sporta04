          "_name": "team1",
          "_customHTML": "<section class=\"team1\" group=\"Team\" plugins=\"amp-main\" mbr-class=\"{'mbr-fullscreen': fullScreen}\">\n    <mbr-parameters>\n        <input type=\"checkbox\" title=\"Полный экран\" name=\"fullScreen\">\n        <input type=\"range\" inline title=\"Верх\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"4\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Низ\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"4\" condition=\"fullScreen == false\">\n        <input type=\"checkbox\" title=\"Главный заголовок\" name=\"showmainTitle\" checked>\n        <input type=\"checkbox\" title=\"Показать имя\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Показать статус\" name=\"showSubTitle\" checked>\n        <input type=\"checkbox\" title=\"Текст\" name=\"showText\">\n        <input type=\"range\" inline title=\"Размер\" name=\"iconsSize\" min=\"1\" max=\"4\" step=\"0.5\" value=\"1.5\">\n        <input type=\"color\" title=\"Цвет\" name=\"iconsColor\" value=\"#ffffff\">\n        <select title=\"Карточки\" name=\"cardsAmount\">\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\" selected>4</option>\n        </select>\n        <select title=\"Значки\" name=\"iconsAmount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\" selected>3</option>\n        </select>\n        <input type=\"image\" name=\"image1\" title=\"Изображение 1\" value=\"../_images/img/img1.jpg\">\n        <input type=\"image\" name=\"image2\" title=\"Изображение 2\" value=\"../_images/img/img2.jpg\" condition=\"cardsAmount > 1\">\n        <input type=\"image\" name=\"image3\" title=\"Изображение 3\" value=\"../_images/img/img3.jpg\" condition=\"cardsAmount > 2\">\n        <input type=\"image\" name=\"image4\" title=\"Изображение 4\" value=\"../_images/img/img4.jpg\" condition=\"cardsAmount > 3\">\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"color\" title=\"Фоновый цвет\" value=\"#ffffff\" selected>\n            <input type=\"image\" title=\"Фоновое изображение\" value=\"../_images/background1.jpg\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Наложение\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Цвет\" name=\"overlayColor\" value=\"#dadada\" condition=\"overlay && bg.type == 'image'\">\n        <input type=\"range\" inline title=\"Прозрачность\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type == 'image'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n    <div class=\"container-fluid\">\n        <h1 class=\"mbr-fonts-style mbr-section-title mbr-bold align-center\" mbr-theme-style=\"display-3\" mbr-if=\"showmainTitle\" data-app-selector=\".mbr-section-title\">НАША КОМАНДА</h1>\n        <div class=\"mbr-row mbr-justify-content-center\">\n            <div class=\"card mbr-col-sm-12 mbr-col-md-12 bg1\" mbr-class=\"{\n                              'mbr-col-lg-6': cardsAmount == 2,                                                \n                             'mbr-col-lg-4': cardsAmount == 3,\n                             'mbr-col-lg-3': cardsAmount == 4,                                             \n                             'mbr-col-lg-2': cardsAmount > 4,\n                             'last-child': cardsAmount == 1}\">\n                <div class=\"card-wrapper\">\n                    <div class=\"card-img\">\n\n                    </div>\n                    <div class=\"card-box align-center\">\n                        <h4 class=\"name mbr-fonts-style mbr-bold align-center mbr-white\" mbr-theme-style=\"display-3\" mbr-if=\"showTitle\" data-app-se