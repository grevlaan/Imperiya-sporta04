          "_name": "footer2",
          "_customHTML": "<section class=\"footer2\" group=\"Footers\" plugins=\"amp-main,amp-form,amp-mustache,amp-iframe\" mbr-class=\"{'mbr-fullscreen': fullScreen}\">\n\n    <mbr-parameters>\n        <input type=\"checkbox\" title=\"Полный экран\" name=\"fullScreen\">\n        <input type=\"range\" inline title=\"Верх\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"4\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Низ\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"2\" condition=\"fullScreen == false\">\n        <input type=\"checkbox\" title=\"Изображение\" name=\"showImg\">\n        <input type=\"checkbox\" title=\"Значки\" name=\"showIcons\" checked>\n        <input type=\"checkbox\" title=\"Заголовок\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Текст\" name=\"showText\" checked>\n        <input type=\"color\" title=\"Цв. рамки\" name=\"borderColor\" value=\"#828282\">\n        <input type=\"range\" title=\"Ширина логотипа\" inline name=\"logoWidth\" min=\"50\" max=\"120\" value=\"88\" step=\"1\">\n        <input type=\"color\" title=\"Цв. значков контактов\" name=\"iconsColorCont\" value=\"#b9b9b9\" condition=\"showIcons\">\n        <input type=\"range\" inline title=\"Размер значков контактов\" name=\"iconsSizeCont\" min=\"0.2\" max=\"2\" step=\"0.1\" value=\"1.4\" condition=\"showIcons\">\n        <input type=\"range\" inline name=\"listItems\" title=\"Список элементов\" min=\"0\" max=\"6\" value=\"6\">\n        <input type=\"checkbox\" title=\"Форма слева/справа\" name=\"reverseContent\">\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"color\" title=\"Фоновый цвет\" value=\"#29282d\" selected>\n            <input type=\"image\" title=\"Фоновое изображение\" value=\"../_images/background1.jpg\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Наложение\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Цвет\" name=\"overlayColor\" value=\"#000000\" condition=\"overlay && bg.type == 'image'\">\n        <input type=\"range\" inline title=\"Прозрачность\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type == 'image'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div class=\"container\">\n\n        <div class=\"mbr-row mbr-justify-content-center main-row\">\n            <div class=\"mbr-col-lg-3 mbr-col-sm-12 padding\">\n                <h3 class=\"mbr-text mbr-bold mbr-fonts-style group-title mbr-white\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".group-title\">FITNESSAMP</h3>\n                <div class=\"img-wrap\" mbr-if=\"showImg\">\n                    <amp-img src=\"../_images/logo.png\" layout=\"responsive\" width=\"90.5311778290993\" height=\"80\" alt=\"Mobirise\">\n                        <img class=\"mbr-temp\" src=\"../_images/logo.png\" alt=\"Mobirise\">\n                    </amp-img>\n                </div>\n                <p class=\"mbr-fonts-style text3\" mbr-theme-style=\"display-5\" data-app-selector=\".text3\" mbr-if=\"showText\">Lorem ipsum dolor sit amet, consectetur adipiscing elit. Sed vehicula blandit mi. Maecenas elementum tincidunt.</p>\n\n\n                <div class=\"contacts-ico\">\n                    <span mbr-icon class=\"mbr-iconfont-c fa-tablet fa\" mbr-if=\"showIcons\"></span>\n                    <h5 class=\"mbr-fonts-style contacts\" mbr-theme-style=\"display-5\" data-app-selector=\".contacts-ico\">+1 (0) 000 0000 001</h5>\n                </div>\n                <div class=\"contacts-ico\">\n                    <span mbr-icon class=\"mbr-iconfont-c fa-map-marker fa\" mbr-if=\"showIcons\"></span>\n                    <h5 class=\"mbr-fonts-style contacts\" mbr-theme-style=\"display-5\" data-ap